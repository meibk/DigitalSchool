{% extends 'TopxiaWebBundle:Parent:layout.html.twig' %}

{% block title %}考试成绩 - {{ parent() }}{% endblock %}

{% set side_nav = 'parent-child-testpapers' %}

{% block main %}
  <div class="panel panel-default panel-col">
    <div class="panel-heading">
      考试成绩
    </div>
    <div class="panel-body">
      <table class="table table-striped table-hover">
        <tbody>
          {% for myTestpaperResult in myTestpaperResults %}
            {% set myTestpaper = myTestpapers[myTestpaperResult.testId]|default(null) %}
            {% if myTestpaper %}
              {% set course = courses[myTestpaper.target|get_course_id] %}
            {% endif %}
            {% include 'TopxiaWebBundle:MyQuiz:my-quiz-tr.html.twig' %}
          {% else %}
            <div class="empty">尚未参与过考试</div>
          {% endfor %}
        </tbody>
      </table>
    </div>
  </div>

{% endblock %}